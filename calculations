System.out.println("enter numbers a ");
int a = s.nextInt();
        System.out.println("enter numbers b ");
        int b = s.nextInt();
        System.out.println("enter numbers c ");
        int c = s.nextInt();
        System.out.println("enter numbers d");
        int d = s.nextInt();
        System.out.println("enter numbers e ");
        int e= s.nextInt();
        int sum = a+b+c+d+e;
        int multi = a*b*c*d*e;
float f;
        f = (a/c )*(b/d) + e;
        System.out.println( "sum is  "+ sum);
        System.out.println("multiple is " + multi);
        System.out.println("expression is "+ f);
        int x = 7;
        int y = 7*49/7 + 35/7;
            System.out.println(y);


